def test ([int] [int] [int] [int] ->):
  -> actualL actualR expectedL expectedR;
  \if (actualL expectedL {(=)} eqVector) not
   || (actualR expectedR {(=)} eqVector) not:
    "Expected:" say
      tab expectedL sayInts
      tab expectedR sayInts
    "But got:" say
      tab actualL sayInts
      tab actualR sayInts
    1 exit

[] 0 splitAt
[] []
test

[0] 0 splitAt
[] [0]
test

[0] 1 splitAt
[0] []
test

[0, 1] 0 splitAt
[] [0, 1]
test

[0, 1] 1 splitAt
[0] [1]
test

[0, 1] 2 splitAt
[0, 1] []
test

[1, 2, 3, 4, 5] -> v;

v -1 splitAt
[] v
test

v (v length / 2) splitAt
[1, 2] [3, 4, 5]
test
