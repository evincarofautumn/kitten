data Example:
  case Example int int;

0 1 Example match: case Example: (+) sayInt

1 2 Example
match:
  case Example:
    -> x y;
    (x + y) sayInt;

3 4 Example
match:
  case Example -> x y:
    (x + y) sayInt;

match (4 5 Example):
  case Example:
    -> x y;
    (x + y) sayInt;

match (5 6 Example):
  case Example -> x y:
    (x + y) sayInt;
