// Converts a character to an integer code point.
define charToInt (char -> int):
  kitten::intrinsic::charToInt

// Converts an integer code point to a character.
define intToChar (int -> char?):
  kitten::intrinsic::intToChar

// Converts an float to int according to C-semantics
define floatToInt(float -> int?):
  kitten::intrinsic::floatToInt

define intToFloat(int -> float?):
  kitten::intrinsic::intToFloat
