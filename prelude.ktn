def readBool ([Char] -> [Bool]):
  -> x
  if x "true" = :
    [true]
  else if x "false" = :
    [false]
  else:
    (Bool)[]

// Show

def show (Bool -> [Char]):
  if: "true" else: "false"

def show (Int -> [Char]):
  __show_int

def show (Float -> [Char]):
  __show_float

def show ([Bool] -> [Char]):
  (Bool -> [Char]){show} showVector

def show ([Int] -> [Char]):
  (Int -> [Char]){show} showVector

def show ([Float] -> [Char]):
  (Float -> [Char]){show} showVector

def show ([[Char]] -> [Char]):
  `id showVector

def showVector ([a] (a -> [Char]) -> [Char]):
  -> show
  -> xs
  "[ "
  xs "" (a [Char] -> [Char]){
    -> acc
    -> value
    value show __apply " " +
    acc +
  } foldUp +
  "]" +

