// Core

def id (a -> a) {}

// Pushes a value to the top of a vector.
def push ([a] a -> [a]):
  vector +

// Prepends a value to the bottom of a vector.
def prepend (a [a] -> [a]):
  -> xs
  -> x
  x vector xs +

def first ((a,) -> a):
  __first

def second ((a, b) -> b):
  __rest __first

def third ((a, b, c) -> c):
  __rest __rest __first

def fourth ((a, b, c, d) -> d):
